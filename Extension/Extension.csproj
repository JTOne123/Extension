<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard1.3;netstandard1.5;net461;net45</TargetFrameworks>
    <IncludeSymbols>True</IncludeSymbols>
    <PackageVersion>1.0.1</PackageVersion>
    <title>C# Linq Extensions</title>
    <Authors>Dominik Lindemann / Axel Katzur</Authors>
    <Description>C# Linq extension with given error messages</Description>
    <Copyright>Copyright 2017 (c). All rights reserved.</Copyright>
    <PackageTags>linq extension error exception NETStandard C# .net</PackageTags>
    <PackageLicenseUrl>http://choosealicense.com/licenses/bsd-2-clause/</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/noobsmuc/Extension</PackageProjectUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <AssemblyName>NoobsMuc.Extension</AssemblyName>
    <RootNamespace>NoobsMuc.Extension</RootNamespace>
    <PackageReleaseNotes>first version</PackageReleaseNotes>
    <PackageIconUrl>https://secure.gravatar.com/avatar/eaa2ca6694b26502955aea7afc3a757d</PackageIconUrl>
  </PropertyGroup>

</Project>
